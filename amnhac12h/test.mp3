<script id="audio-recommend-template" type="text/template">
    <% _.each( items, function(item) {%>    
    <li id="songrec<%= item.id %>" class="fn-item" data-type="song" data-id="<%= item.id %>" data-name="<%= item.name %>" data-artist="<%= item.artists_names %>" data-code="<%= item.code %>" data-active="show-tool">
        <a href="<%= item.link %>" title="Bài hát <%= item.name %> - <%= item.artists_names %>" class="thumb fn-link _trackLink track-log">
            <img width="50" height="50" class="fn-thumb" src="<%= item.thumbnail %>" alt="Bài hát <%= item.name %> - <%= item.artists_names %>" />
            <span class="icon-circle-play icon-small"></span>
        </a>                    
        <h3 class="song-name ellipsis-2">
            <a href="<%= item.link %>" title="Bài hát <%= item.name %> - <%= item.artists_names %>" class="txt-primary fn-link fn-name _trackLink track-log">
                <%= item.name %>
            </a>
        </h3>
        <% if (item.artists) { %>
            <div class="inblock ellipsis fn-artist_list">
            <% _.each( item.artists, function(artist , index) {%>   
                <h4 class="singer-name txt-info fn-artist"><a href="<%= artist.link %>" title="<%= artist.name %>"><%= artist.name %></a></h4><% if (index !== item.artists.length - 1) { %><span class="ft">, </span><% } %>
            <% }); %>
    </div>
    <% } %>
    <div class="tool-song log-vote-recommend">
    <div class="i25 i-small download"><a title="Tải về" class="fn-dlsong" data-item="#songrec<%= item.id %>" href="#"></a></div>
    <div class="i25 i-small addlist"><a title="Thêm vào" class="fn-addto" data-item="#songrec<%= item.id %>" href="#"></a></div>
    <div class="i25 i-small share"><a title="Chia sẻ" class="fn-share" data-item="#songrec<%= item.id %>" href="https://mp3.zing.vn<%= item.link %>"></a></div>        
    </div>
    </li>
    <% }); %>
</script>



